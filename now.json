{
  "name": "serverless-now-node",
  "version": 2,
  "alias": "akhilome-snn",
  "env": {
    "DB_URL": "@snn_db_url"
  },
  "builds": [
    { "src": "api/**/*.ts", "use": "@now/node" },
    { "src": "index.ts", "use": "@now/node" },
    { "src": "404/index.ts", "use": "@now/node" }
  ],
  "regions": ["lhr1", "dub1"],
  "routes": [{ "handle": "filesystem" }, { "src": "/(.*)", "dest": "/404" }]
}
